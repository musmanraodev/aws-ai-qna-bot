  "scripts": {
    "test-unit": "./node_modules/karma/bin/karma start website/test/unit/karma.conf.js",
    "test-e2e-remote": "export URL=$(./bin/URL.sh) && cd website/test/e2e && ./runner.sh ",
    "check": "./bin/check.sh",
    "stack": "./bin/launch.sh",
    "upload": "make upload -j 4",
    "bootstrap": "make config.js && make templates && npm run stack dev/bootstrap up",
    "dev-up": "npm run stack dev/dev up",
    "up": "npm run upload && npm run stack test/master up",
    "lambda-update": "make lambda && ./bin/update-lambda.sh",
    "admin-update": "npm run build && ./bin/update-admin.sh"
  },
